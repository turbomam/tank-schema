{
  "name": "tank",
  "comments": [
    "corresponds to schema2.yaml from Silvano Cirujano Cuesta",
    "https://obo-communitygroup.slack.com/archives/C04EU7JL1NF/p1687194459249399"
  ],
  "id": "http://example.org/tank",
  "prefixes": {
    "linkml": {
      "prefix_prefix": "linkml",
      "prefix_reference": "https://w3id.org/linkml/"
    },
    "xsd": {
      "prefix_prefix": "xsd",
      "prefix_reference": "http://www.w3.org/2001/XMLSchema#"
    },
    "shex": {
      "prefix_prefix": "shex",
      "prefix_reference": "http://www.w3.org/ns/shex#"
    },
    "schema": {
      "prefix_prefix": "schema",
      "prefix_reference": "http://schema.org/"
    }
  },
  "default_prefix": "http://example.org/tank/",
  "default_range": "string",
  "types": {
    "string": {
      "name": "string",
      "description": "A character string",
      "from_schema": "http://example.org/tank",
      "exact_mappings": [
        "schema:Text"
      ],
      "base": "str",
      "uri": "xsd:string"
    },
    "integer": {
      "name": "integer",
      "description": "An integer",
      "from_schema": "http://example.org/tank",
      "exact_mappings": [
        "schema:Integer"
      ],
      "base": "int",
      "uri": "xsd:integer"
    },
    "boolean": {
      "name": "boolean",
      "description": "A binary (true or false) value",
      "from_schema": "http://example.org/tank",
      "exact_mappings": [
        "schema:Boolean"
      ],
      "base": "Bool",
      "uri": "xsd:boolean",
      "repr": "bool"
    },
    "float": {
      "name": "float",
      "description": "A real number that conforms to the xsd:float specification",
      "from_schema": "http://example.org/tank",
      "exact_mappings": [
        "schema:Float"
      ],
      "base": "float",
      "uri": "xsd:float"
    },
    "double": {
      "name": "double",
      "description": "A real number that conforms to the xsd:double specification",
      "from_schema": "http://example.org/tank",
      "close_mappings": [
        "schema:Float"
      ],
      "base": "float",
      "uri": "xsd:double"
    },
    "decimal": {
      "name": "decimal",
      "description": "A real number with arbitrary precision that conforms to the xsd:decimal specification",
      "from_schema": "http://example.org/tank",
      "broad_mappings": [
        "schema:Number"
      ],
      "base": "Decimal",
      "uri": "xsd:decimal"
    },
    "time": {
      "name": "time",
      "description": "A time object represents a (local) time of day, independent of any particular day",
      "notes": [
        "URI is dateTime because OWL reasoners do not work with straight date or time"
      ],
      "from_schema": "http://example.org/tank",
      "exact_mappings": [
        "schema:Time"
      ],
      "base": "XSDTime",
      "uri": "xsd:dateTime",
      "repr": "str"
    },
    "date": {
      "name": "date",
      "description": "a date (year, month and day) in an idealized calendar",
      "notes": [
        "URI is dateTime because OWL reasoners don't work with straight date or time"
      ],
      "from_schema": "http://example.org/tank",
      "exact_mappings": [
        "schema:Date"
      ],
      "base": "XSDDate",
      "uri": "xsd:date",
      "repr": "str"
    },
    "datetime": {
      "name": "datetime",
      "description": "The combination of a date and time",
      "from_schema": "http://example.org/tank",
      "exact_mappings": [
        "schema:DateTime"
      ],
      "base": "XSDDateTime",
      "uri": "xsd:dateTime",
      "repr": "str"
    },
    "date_or_datetime": {
      "name": "date_or_datetime",
      "description": "Either a date or a datetime",
      "from_schema": "http://example.org/tank",
      "base": "str",
      "uri": "linkml:DateOrDatetime",
      "repr": "str"
    },
    "uriorcurie": {
      "name": "uriorcurie",
      "description": "a URI or a CURIE",
      "from_schema": "http://example.org/tank",
      "base": "URIorCURIE",
      "uri": "xsd:anyURI",
      "repr": "str"
    },
    "curie": {
      "name": "curie",
      "conforms_to": "https://www.w3.org/TR/curie/",
      "description": "a compact URI",
      "comments": [
        "in RDF serializations this MUST be expanded to a URI",
        "in non-RDF serializations MAY be serialized as the compact representation"
      ],
      "from_schema": "http://example.org/tank",
      "base": "Curie",
      "uri": "xsd:string",
      "repr": "str"
    },
    "uri": {
      "name": "uri",
      "conforms_to": "https://www.ietf.org/rfc/rfc3987.txt",
      "description": "a complete URI",
      "comments": [
        "in RDF serializations a slot with range of uri is treated as a literal or type xsd:anyURI unless it is an identifier or a reference to an identifier, in which case it is translated directly to a node"
      ],
      "from_schema": "http://example.org/tank",
      "close_mappings": [
        "schema:URL"
      ],
      "base": "URI",
      "uri": "xsd:anyURI",
      "repr": "str"
    },
    "ncname": {
      "name": "ncname",
      "description": "Prefix part of CURIE",
      "from_schema": "http://example.org/tank",
      "base": "NCName",
      "uri": "xsd:string",
      "repr": "str"
    },
    "objectidentifier": {
      "name": "objectidentifier",
      "description": "A URI or CURIE that represents an object in the model.",
      "comments": [
        "Used for inheritence and type checking"
      ],
      "from_schema": "http://example.org/tank",
      "base": "ElementIdentifier",
      "uri": "shex:iri",
      "repr": "str"
    },
    "nodeidentifier": {
      "name": "nodeidentifier",
      "description": "A URI, CURIE or BNODE that represents a node in a model.",
      "from_schema": "http://example.org/tank",
      "base": "NodeIdentifier",
      "uri": "shex:nonLiteral",
      "repr": "str"
    }
  },
  "slots": {
    "wheels": {
      "name": "wheels",
      "from_schema": "http://example.org/tank",
      "required": true
    },
    "chains": {
      "name": "chains",
      "from_schema": "http://example.org/tank",
      "is_a": "wheels",
      "required": true
    },
    "tanks": {
      "name": "tanks",
      "comments": [
        "from @turbomam, for better compatibility with examples-first-cookiecutter"
      ],
      "from_schema": "http://example.org/tank",
      "multivalued": true,
      "range": "Tank",
      "inlined": true,
      "inlined_as_list": true
    }
  },
  "classes": {
    "Vehicle": {
      "name": "Vehicle",
      "from_schema": "http://example.org/tank",
      "slots": [
        "wheels"
      ],
      "attributes": {
        "wheels": {
          "name": "wheels",
          "from_schema": "http://example.org/tank",
          "alias": "wheels",
          "owner": "Vehicle",
          "domain_of": [
            "Vehicle"
          ],
          "range": "string",
          "required": true
        }
      }
    },
    "Tank": {
      "name": "Tank",
      "from_schema": "http://example.org/tank",
      "is_a": "Vehicle",
      "slots": [
        "chains"
      ],
      "slot_usage": {
        "wheels": {
          "name": "wheels",
          "comments": [
            "{\"it does not seem like the 'required\": \"false' usage is being honored\"}"
          ],
          "domain_of": [
            "Vehicle"
          ],
          "required": false
        },
        "chains": {
          "name": "chains",
          "domain_of": [
            "Tank"
          ],
          "required": true
        }
      },
      "attributes": {
        "chains": {
          "name": "chains",
          "from_schema": "http://example.org/tank",
          "is_a": "wheels",
          "alias": "chains",
          "owner": "Tank",
          "domain_of": [
            "Tank"
          ],
          "range": "string",
          "required": true
        },
        "wheels": {
          "name": "wheels",
          "comments": [
            "{\"it does not seem like the 'required\": \"false' usage is being honored\"}"
          ],
          "from_schema": "http://example.org/tank",
          "alias": "wheels",
          "owner": "Tank",
          "domain_of": [
            "Vehicle"
          ],
          "range": "string",
          "required": false
        }
      }
    },
    "TankCollection": {
      "name": "TankCollection",
      "comments": [
        "from @turbomam, for better compatibility with examples-first-cookiecutter"
      ],
      "from_schema": "http://example.org/tank",
      "slots": [
        "tanks"
      ],
      "attributes": {
        "tanks": {
          "name": "tanks",
          "comments": [
            "from @turbomam, for better compatibility with examples-first-cookiecutter"
          ],
          "from_schema": "http://example.org/tank",
          "multivalued": true,
          "alias": "tanks",
          "owner": "TankCollection",
          "domain_of": [
            "TankCollection"
          ],
          "range": "Tank",
          "inlined": true,
          "inlined_as_list": true
        }
      }
    }
  },
  "source_file": "src/tank_schema/schema/tank_schema.yaml",
  "@type": "SchemaDefinition"
}
